    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.10.0",
    "jsdom": "^9.11.0",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.1",
    "react-addons-test-utils": "^15.4.2",
    "sass-loader": "^6.0.1",
    "sinon": "^1.17.7",
