{
  "name": "foursquare-search",
  "version": "0.0.1",
  "description": "An app to search for recommended and popular places using the FourSquare API",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/concurrently -p \"[{name}]\" -n \"SERVER,WEPBACK-WATCH\" -c \"black.bgMagenta.bold,black.bgGreen.bold\" \"npm run start:server\" \"npm run build:react -- -w\"",
    "start:server": "NODE_ENV=development npm run build:server && node ./lib/index.js",
    "build:react": "NODE_ENV=development ./node_modules/.bin/webpack",
    "build:server": "./node_modules/.bin/babel src/server -d lib/",
    "test": "./node_modules/.bin/mocha --require testSetup.js --compilers js:babel-register test/**/*.test.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.1",
    "body-parser": "^1.16.1",
