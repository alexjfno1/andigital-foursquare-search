{
  "extends": "airbnb",
  "plugins": [
    "chai-expect",
    "mocha"
  ],
  "rules": {
    "comma-dangle": ["error", "never"],
    "react/jsx-filename-extension": [0]
  },
  "globals": {
    "document": true,
    "window": true,
    "describe": true,
    "it": true,
    "expect": true,
    "shallow": true,
    "sinon": true
  }
}
